<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="ru.nsu.ccfit.kokunina.controllers.GameListController"
      spacing="10"
      stylesheets="@styles/app.css">
    <Text>Existing games</Text>
    <ListView fx:id="listView" prefHeight="400.0" prefWidth="600.0"/>
    <Button fx:id="joinGameButton" onAction="#handleJoinGameButton" visible="false">Join game</Button>
    <Button fx:id="backButton" onAction="#handleBackButton">Back</Button>

</VBox>
