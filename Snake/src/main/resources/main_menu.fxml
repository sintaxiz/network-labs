<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="ru.nsu.ccfit.kokunina.controllers.MainMenuController"
      prefHeight="400.0" prefWidth="600.0"
      alignment="CENTER"
      spacing="20"
      stylesheets="@styles/app.css">

    <Button fx:id="newGameButton" text="NEW GAME" onAction="#startNewGame"/>
    <Button fx:id="connectGameButton" text="JOIN GAME" onAction="#connectExistingGame"/>

</VBox>
